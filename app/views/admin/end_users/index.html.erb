<h2 class="head-line title_h2 text-center">ユーザー一覧</h2>
<div class="row">
    <table class="table">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>name</th>
          <th>email</th>
          <th>ステータス</th>
        </tr>
      </thead>

      <tbody>
        <% @end_users.each do |end_user| %>
          <td><%= end_user.id %></td>
          <td>
            <%= link_to admin_end_user_path(end_user), class: "btn-link"  do %>
              <%= end_user.name %>
            <% end %>
          </td>
          <td><%= end_user.email %></td>
          <td>
            <% if end_user.is_deleted == true%>
              <b class="text-danger">退会済</b>
            <% else %>
              <b class="text-success">有効</b>
            <% end %>
          </td>
      </tbody>
        <% end %>
    </table>
</div>