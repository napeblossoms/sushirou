 <table class= 'table table-hover table-inverse table-light'>
        <thead class= "thead-light">
          <tr>
            <th>旬</th>
            <th>ネタ</th>
            <th>メモ</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
        <% @dictionarys.each do |dictionary| %>
         <tr>
           <td><%= dictionary.season %></td>
           <td>
             <%= link_to admin_dictionary_path(dictionary.id) do %>
              <%= dictionary.name %>
             <% end %>
           </td>
           <td><%= dictionary.body %></td>
         </tr>
        </tbody>
       <% end %>
  </table>