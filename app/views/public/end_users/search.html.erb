<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% if @end_users.exists? %>
      <% @end_users.each do |end_user| %>
        <tr>
          <td><%= image_tag end_user.get_profile_image, width: "50px",height: "50px",class:"rounded-circle"; %></td>
          <td>
            <%= link_to  public_end_user_path(end_user.id) do %>
              <%= end_user.name %>
            <% end %>
          </td>
        </tr>
      <% end %>
    <% else %>
      <p>ユーザーはいません</p>
    <% end %>
  </tbody>
</table>
